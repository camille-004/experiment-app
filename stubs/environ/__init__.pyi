from typing import Any

class Env:
    def __init__(self, **scheme: Any) -> None: ...
    def read_env(self, env_file: str | None = None, **overrides: Any) -> None: ...
    def str(self, var: str, default: str | None = ...) -> str: ...
    def bool(self, var: str, default: bool | None = ...) -> bool: ...
    def int(self, var: str, default: int | None = ...) -> int: ...
    def list(self, var: str, default: list[str] | None = ...) -> list[str]: ...
    def db(self, var: str, default: str | None = ...) -> dict[str, Any]: ...
